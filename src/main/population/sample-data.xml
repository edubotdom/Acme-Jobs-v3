<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />

	<!-- Shouts -->

	<bean id="shout_1" class="acme.entities.shouts.Shout">
		<property name="author" value="default name"></property>
		<property name="text" value="default text"></property>
		<property name="moment" value="2019/09/09 09:00"></property>
	</bean>
	
	
	<bean id="shout_2" class="acme.entities.shouts.Shout">
		<property name="author" value="default name"></property>
		<property name="text" value="default text"></property>
		<property name="moment" value="2019/09/10 10:00"></property>
	</bean>
	

</beans>
